const url = window.location.href;
const apiUrl = `https://count-server.sharethis.com/v2.0/get_counts?url=${encodeURIComponent(url)}`;

fetch(apiUrl)
    .then(response => response.json())
    .then(data => {
        // Extract share count data from the response
        const shareCount = data?.shares?.all || 0;
        console.log('Share Count:', shareCount);

        // Call the function to display share count only if the sharesContainer element exists
        const shareCountElement = document.getElementById('shareCount');
        if (shareCountElement) {
            displayShareCount(shareCount);
        }
    })
    .catch(error => {
        console.error('Error fetching share count:', error);
    });

	// Fetch share count and display it
	function displayShareCount(count) {
		if(count >= 25) {
			document.getElementById('sharesContainer').style.display = "block";
		} 
		document.getElementById('shareCount').innerText = count;
	}
